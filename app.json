{
  "expo": {
    "name": "VinylVectors",
    "slug": "VinylVectors",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-asset",
        {
          "assets": ["./assets/images"]
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "VinylVectors needs access to your Camera."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#e69d50"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.invectory.VinylVectors",
      "infoPlist": {
        "NSCameraUsageDescription": "VinylVectors requires access to the camera to take photos of album covers."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e0caa2"
      },
      "package": "app.invectory.VinylVectors"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}